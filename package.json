{
  "name": "hasura-actions-ts",
  "version": "1.0.0",
  "description": "HACT - Hasura Actions API",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules --poll src/index.ts",
    "build": "rimraf ./dist && npm run test && tsc",
    "postbuild": "cpy './src/config/*.json' ./dist/config/",
    "start": "npm run build && node dist/index.js",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register './src/tests/**/*.ts'",
    "lint": "eslint --fix . --ext .ts",
    "prettier-format": "run-script-os",
    "prettier-format:win32": "prettier --config .prettierrc \"./src/**/*.ts\" --write",
    "prettier-format:darwin:linux": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-format:default": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-generator": "run-script-os",
    "prettier-generator:win32": "prettier --config .prettierrc \"./src/generated/*.ts\" --write",
    "prettier-generator:darwin:linux": "prettier --config .prettierrc 'src/generated/*.ts' --write",
    "prettier-generator:default": "prettier --config .prettierrc 'src/generated/*.ts' --write",
    "prettier-route": "run-script-os",
    "prettier-route:win32": "prettier --config .prettierrc \"./src/routes/*.ts\" --write",
    "prettier-route:darwin:linux": "prettier --config .prettierrc 'src/routes/*.ts' --write",
    "prettier-route:default": "prettier --config .prettierrc 'src/routes/*.ts' --write",
    "err:cli": "ts-node system/cli/err.cli.ts",
    "route:cli": "ts-node system/cli/route.cli.ts && npm run prettier-route",
    "generate:schema": "rimraf ./graphql/schema.graphql && rimraf ./graphql/operations && graphql-codegen --config codegen-schema.js",
    "generate:all": "graphql-codegen --config codegen.js && npm run prettier-generator",
    "hact:all": "npm run generate:schema && node generator.js && npm run generate:all",
    "rebuild:dev": "docker compose -f docker-compose.yml rm --stop --force  && docker compose -f docker-compose.yml build --no-cache && docker compose -f docker-compose.yml up",
    "prepare": "husky install",
    "commit": "cz",
    "hasura:up": "docker compose -f hasura-docker-compose.yml up -d",
    "hasura:down": "docker compose -f hasura-docker-compose.yml down --remove-orphans"
  },
  "keywords": [],
  "author": "Kristijan Klepaƒç <kklepac@iolap.com>",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.2",
    "convict": "^6.2.3",
    "convict-format-with-validator": "^6.2.0",
    "cpy-cli": "^4.2.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "figlet": "^1.5.2",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "got": "^11.8.5",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0",
    "graphql-tag": "^2.12.6",
    "http-terminator": "^3.2.0",
    "husky": "^8.0.1",
    "inquirer": "^8.2.4",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6",
    "slugify": "^1.6.5",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/cli": "^2.13.7",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "^2.7.4",
    "@graphql-codegen/typescript-graphql-request": "^4.5.6",
    "@graphql-codegen/typescript-operations": "^2.5.4",
    "@types/chai": "^4.3.3",
    "@types/convict": "^6.1.1",
    "@types/convict-format-with-validator": "^6.0.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/figlet": "^1.5.5",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^8.0.0",
    "@types/inquirer": "^9.0.2",
    "@types/lodash": "^4.14.186",
    "@types/mocha": "^10.0.0",
    "@types/morgan": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "chai": "^4.3.6",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lint-staged": "^13.0.3",
    "mocha": "^10.1.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
